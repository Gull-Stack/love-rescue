{
  "outputDirectory": "landing",
  "rewrites": [
    { "source": "/services/:path*", "destination": "/services/:path*.html" },
    { "source": "/blog/:path*", "destination": "/blog/:path*.html" },
    { "source": "/login", "destination": "https://marriage-rescue-frontend.vercel.app/login" },
    { "source": "/signup", "destination": "https://marriage-rescue-frontend.vercel.app/signup" },
    { "source": "/dashboard/:path*", "destination": "https://marriage-rescue-frontend.vercel.app/dashboard/:path*" },
    { "source": "/settings/:path*", "destination": "https://marriage-rescue-frontend.vercel.app/settings/:path*" },
    { "source": "/assessments/:path*", "destination": "https://marriage-rescue-frontend.vercel.app/assessments/:path*" },
    { "source": "/join/:path*", "destination": "https://marriage-rescue-frontend.vercel.app/join/:path*" },
    { "source": "/admin/:path*", "destination": "https://marriage-rescue-frontend.vercel.app/admin/:path*" }
  ],
  "cleanUrls": true
}
